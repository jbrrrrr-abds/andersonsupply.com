{
  "name": "next-starter",
  "version": "1.0.0",
  "scripts": {
    "dev": "NODE_OPTIONS=--openssl-legacy-provider next",
    "build": "NODE_OPTIONS=--openssl-legacy-provider next build",
    "export": "NODE_OPTIONS=--openssl-legacy-provider yarn build && next export",
    "postexport": "node scripts/getSitemap.js",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint './src/**/*.js'",
    "lint-staged": "lint-staged",
    "test": "jest"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "stylelint",
      "jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.0",
    "@prismicio/client": "^4.0.0",
    "axios": "^1.6.7",
    "babel-plugin-styled-components": "^1.10.0",
    "date-fns": "^2.1.0",
    "dotenv": "^8.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "flickity": "^2.2.2",
    "gsap": "./gsap/gsap-bonus.tgz",
    "next": "^14.1.0",
    "polished": "^3.4.1",
    "prismic-reactjs": "^1.3.3",
    "prop-types": "^15.6.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^6.0.0",
    "react-hook-form": "^7.2.3",
    "react-intersection-observer": "^8.24.1",
    "react-modal": "^3.13.1",
    "react-scrolllock": "^5.0.1",
    "react-transition-group": "^4.4.1",
    "styled-components": "^5.1.0",
    "superior-mq": "^1.0.1",
    "use-is-tabbing": "^0.1.1",
    "use-onclickoutside": "^0.3.2",
    "zustand": "^3.4.2"
  },
  "devDependencies": {
    "@gulls/stylelint-config": "^0.1.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "@ueno/eslint-config": "^1.3.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.23.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "jest": "^29.7.0",
    "lint-staged": "^7.0.0",
    "pre-commit": "^1.2.2",
    "stylelint": "^16.2.1"
  },
  "eslintIgnore": [
    "public/**",
    "out"
  ],
  "license": "UNLICENSED",
  "engines": {
    "node": ">=10.15"
  }
