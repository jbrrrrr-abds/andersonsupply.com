    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-styled-components": "^0.0.0",
    "eslint-plugin-styled-components-a11y": "^2.2.1",
    "gsap": "file:gsap/gsap-bonus.tgz",
    "next": "16.0.7",
    "polished": "^4.3.1",
    "prettier": "^3.7.4",
